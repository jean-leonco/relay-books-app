    "database:seed": "babel-node webpackx.js ./scripts/seed/seedDatabase",
    "graphql": "webpack --progress --config webpack.graphql.js",
    "serve:graphql": "node dist/graphql/index.js",
    "test": "jest --maxWorkers=4",
    "test:watch": "jest --watch",
    "update-schema": "babel-node webpackx.js ./scripts/updateSchema.ts && yarn copy-to-project"
  },
